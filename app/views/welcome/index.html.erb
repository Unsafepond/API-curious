<%= render "layouts/navbar" %>
<br>
<br>
<div class="well">
	<% if @user %>
		<%= form_for :tweet, url: tweets_path do |f| %>
		<%= f.text_area :text %>
		<br>
		<%= f.submit "Notweet!" %>
		<% end %>
		<br>
		<%= render "layouts/tweet_feed"  %>
	<% else %>
		<%= render "layouts/twitter_button" %>
	<% end %>
</div>